urxvt*foreground: white
urxvt*background: black
urxvt*urgentOnBell: true
urxvt*scrollBar: false

*color0:  #2E3436
*color1:  #a40000
*color2:  #4E9A06
*color3:  #C4A000
*color4:  #3465A4
*color5:  #75507B
*color6:  #069773
*color7:  #BABDB9
*color8:  #555753
*color9:  #EF2929
*color10: #8AE234
*color11: #FCE94F
*color12: #729FCF
*color13: #AD7FA8
*color14: #34E2E2
*color15: #EEEEEC

urxvt*font: xft:AnonymousPro:size=13,xft:NotoEmoji:pixelsize=10,xft:Code2000

Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold 
URxvt.colorUL: #4682B4

URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0
